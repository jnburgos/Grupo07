<p id="notice"><%= notice %></p>

<h1>Show Subscriptions</h1>

<table>
  <thead>
    <tr>
      <th>Show</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @show_subscriptions.each do |show_subscription| %>
      <tr>
        <td><%= show_subscription.show %></td>
        <td><%= show_subscription.user %></td>
        <td><%= link_to 'Show', show_subscription %></td>
        <td><%= link_to 'Edit', edit_show_subscription_path(show_subscription) %></td>
        <td><%= link_to 'Destroy', show_subscription, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Show Subscription', new_show_subscription_path %>
